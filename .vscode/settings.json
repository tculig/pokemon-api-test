{
  "eslint.validate": [
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "files.associations": {
  "*.css": "tailwindcss"
}
}